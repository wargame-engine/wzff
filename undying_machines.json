    "rules": {
        "plasma_injector": {
            "name": "Plasma Injector",
            "description": "Once per round, this unit may re-roll a single failed Strength test.",
            "points": 5
        },
        "machine_tactics": {
            "name": "Machine Tactics",
            "inputs": ["x"],
            "description": "When this unit is activated, choose up to X friendly _Infantry_ units within 6\" to receive one of the following bonuses until the end of the round: \n * Re-roll failed Strength tests of 9+ \n * Re-roll failed Accuracy tests of 9+",
            "points": ["rule.x", {"multiply": 10}]
        },
        "warden_tactics": {"name": "Warden", "description": "Once per round, a friendly _Infantry_ unit within 6\" may gain +2 Initiative for a skill test.", "points": 5},
        "reanimation_beam": {
            "name": "Reanimation Beam",
            "description": "Once per round after taking Saves for an _Infantry_ unit. Roll a D10 for each model or Wound(x) lost. On a 5 or less, that model or Wound(x) is restored. Models must be restored to full Wounds(x) before restoring a new model.",
            "points": 20
        },
        "targeting_relay": {
            "name": "Targeting Relay",
            "description": "If this unit hits an enemy unit with a ranged attack, mark that unit. The next friendly unit to target that enemy unit may re-roll failed Accuracy rolls of 9+.",
            "points": 10
        },
        "swarm_fabricator": {
            "name": "Swarm Fabricator",
            "description": "Once during this unit's activation, roll 3D10. For each roll of 5 or less, one Machine Swarm model equipped with (Feeding Teeth) and specials (Resilient(2), Hover) is created within 3\" of this unit. They may be added to a friendly Machine Swarm unit within 3\" or a new unit may be created.",
            "points": 50
        },
        "extra_space": {"name": "Extra Space", "description_short": "Transport +10", "hidden": true, "description": "This model gains +10 Transport space.", "points": 10},
        "flare_shielding": {"name": "Flare Shields", "description": "Subtract 1 from all Damage Table(X) rolls for this model.", "points": 5},
        "tesla": {"name": "Tesla", "description": "Each successful hit roll of 1 or 2 with this weapon causes 2 hits.", "points": ["weapon.attacks", {"multiply": 2}]},
        "regeneration_orb": {
            "name": "Regeneration Orb",
            "description": "Once per round after taking Saves for an _Infantry_ unit. Roll a D10 for each model or Wound(x) lost. On a 5 or less, that model or Wound(x) is restored. Models must be restored to full Wounds(x) before restoring a new model.",
            "points": 20
        },
        "regeneration_pods": {
            "name": "Regeneration Pods",
            "description": "Once per round after taking Saves for an _Infantry_ unit. Roll a D10 for each model or Wound(x) lost. On a 5 or less, that model or Wound(x) is restored. Models must be restored to full Wounds(x) before restoring a new model.",
            "points": 20
        },
        "phase_field": {"name": "Phase Field", "description": "Once per round, a friendly _Infantry_ unit within 6\" may gain +1 Save.", "points": 10},
        "anti_gravity_generators": {
            "name": "Anti-Grav Generators",
            "hidden": true,
            "description_short": "+3\" Move, Fly",
            "description": "This model gains +3\" of Movement and the Fly special rule.",
            "points": 5
        },
        "flight_cloak": {"name": "Flight Cloak", "hidden": true, "description_short": "+3\" Move, Fly", "description": "This model gains +3\" of Movement and the Fly special rule.", "points": 5},
        "void_shield": {"name": "Void Shield", "description": "Whenever this model loses a wound, roll a D10. On a 3 or less that wound is not lost.", "points": 5},
        "vision_scope": {"name": "Vision Scope", "description": "This unit's weapons ignore cover at Short range.", "points": 5},
        "snare": {"name": "Snare", "description": "A model may make one free attack with this weapon against units that fall back from combat with it.", "points": 2}
    },
    "strategies": [
        {
            "name": "Fearless",
            "phase": "activation",
            "description": "Use before activating a unit. That unit may immediately remove up to 2 Shock.",
            "flavor": "With cold an unyielding metal, they fear no one.",
            "cost": 1
        },
        {
            "name": "Reanimation",
            "phase": "activation",
            "description": "Use after an attack is completed against an _Infantry_ or _Heavy Infantry_ unit. Roll a D10 for each model or Wound(x) lost. On a 3 or less, that model or Wound(x) is restored. Models must be restored to full Wounds(x) before restoring a new model.",
            "flavor": "Even when they appear to be fully destroyed, their regeneration may kick in and they will live again.",
            "cost": 2
        },
        {
            "name": "Repair Overdrive",
            "phase": "activation",
            "description": "Use when activating a _Vehicle_ unit. It may immediately fix a critical damage it has sustained.",
            "flavor": "With self-repairing nano-machines this vehicle can recover from nearly anything.",
            "cost": 2
        },
        {
            "subfactions": ["solar_fire"],
            "name": "Distruption Flare",
            "phase": "activation",
            "description": "Use before a unit performs a Shoot action. The target reduces its cover level by 1 (hard to soft, soft to none).",
            "flavor": "A solar flare from the sky disrupts the enemy's footing.",
            "cost": 1
        },
        {
            "subfactions": ["solar_fire"],
            "name": "Overwhelming Firewpoer",
            "phase": "activation",
            "description": "Use before an _Infantry_ unit performs a Shoot action. Hit rolls of 1 or 2 cause an additional hit.",
            "flavor": "Massed firepower can be relatively effective in taking down most targets.",
            "cost": 1
        },
        {
            "subfactions": ["solar_fire"],
            "name": "Energy Amplificiation",
            "phase": "activation",
            "description": "Use before a unit performs a Shoot action. All attacks are treated as being at close range.",
            "flavor": "Amplifying the power of an energy weapon can cause it to reach out and hit enemies at great distances.",
            "cost": 2
        },
        {
            "subfactions": ["combat_focus"],
            "name": "Stalk",
            "phase": "pre_game",
            "description": "Up to half of any force's units, one point for each may be placed in Ambush before the game begins. Any round after the first, when this unit is activated while in reserves, you may choose a point on the battlefield 12\" away from any enemy units and take an Initiative check. If passed, place the unit at that location on the location. If failed, use D10 scattering to determine the landing location of the unit relative to the chosen point.",
            "cost": "1/per"
        },
        {
            "subfactions": ["combat_focus"],
            "name": "Murderous Charge",
            "phase": "activation",
            "description": "Use when a unit declares a Charge action. That unit gets +3\" of Movement for that action.",
            "cost": 1
        },
        {
            "subfactions": ["combat_focus"],
            "name": "Rend",
            "phase": "activation",
            "flavor": "It's about time we got to fighting.",
            "description": "Use before a unit performs a Fight action. Hit rolls of 1 or 2 get +1 AP.",
            "cost": 1
        }
    ],
