    "rules": {
        "stealth_field": {"name": "Stealth Field", "description": "For each model with this rule, enemy units targeting this unit suffer -1 Accuracy.", "points": 20},
        "pulse_shield": {"name": "Pulse Shield", "description": "Once per round, roll a die for each hit this unit takes. On a 3 or less that hit is prevented. This may be used one additional time per round for each model with this rule.", "points": 20},
        "commander_tactics": {
            "name": "Commander Tactics",
            "inputs": ["x"],
            "description": "When this unit is activated, choose up to X friendly _Infantry_ or _Heavy Infantry_ units within 6\" to receive one of the following bonuses until the end of the round: \n * Re-roll failed Accuracy tests of 9+ \n * Re-roll failed Initiative tests of 9+",
            "points": ["rule.x", {"multiply": 10}]
        },
        "warrior_tactics": {
            "name": "Warrior Tactics",
            "inputs": ["x"],
            "description": "When this unit is activated, choose up to X friendly _Infantry_ units within 6\" to receive one of the following bonuses until the end of the round: \n * Re-roll failed Accuracy tests of 9+ \n * Re-roll failed Initiative tests of 9+",
            "points": ["rule.x", {"multiply": 10}]
        },
        "savage_tactics": {
            "name": "Savage Tactics",
            "inputs": ["x"],
            "description": "When this unit is activated, choose up to X friendly _Savages_ _Infantry_ units within 6\" to receive one of the following bonuses until the end of the round: \n * Re-roll failed Accuracy tests of 9+ \n * Re-roll failed Strength tests of 9+",
            "points": ["rule.x", {"multiply": 10}]
        },
        "attached": {"name": "Attached", "description": "This model is attached to its hosting unit and counts as being embarked but does not take up any transport space."},
        "spotting_laser": {
            "name": "Spotting Laser",
            "description": "Any time during this model's activation, pick an enemy unit within 36\" and roll a D10. On a 5 or less the enemy unit gains 1 spotting marker. When a friendly unit shoots at that target they may spend 1 spotting marker to re-roll hit rolls of 9+ against that target or 2 spotting markers to get +1 to hit.",
            "points": 5
        },
        "homing_beacon": {"name": "Homing Beacon", "description": "Friendly units deploying using Ambush deployment may ignore enemy units when deploying within 6\" of this model.", "points": 5},
        "inhibitor": {"name": "Inhibitor", "description": "Enemy units suffer -2\" of movement with any Charge actions targeting friendly units within 3\".", "points": 5},
        "tactical_drone": {
            "name": "Tactical Drone",
            "description": "When this unit is activated, choose one ability: One enemy unit within 12\" may not benefit from cover this round. Choose a unit within 2\" and roll one die for each drone. On any results of 5 or less the unit may heal 1 wound. No more than 3 wounds may be healed in one activation.",
            "points": 5
        },
        "recon_drone": {"name": "Recon Scope", "description": "Models in this unit may ignores cover bonuses.", "points": 10},
        "accelerator": {"name": "Accelerator", "description": "This model and models in its unit add +6\" maximum Range to all weapons.", "points": 5},
        "threat_detector_suit": {"name": "Threat Detector", "description_short": "+1 Initiative", "description": "This model gets +1 Initiative.", "points": 5},
        "hover_drone": {"name": "Hover Drone", "hidden": "true", "description": "This model gets +2\" Move and Fly.", "description_short": "+4\" Move, Fly", "points": 5},
        "protection": {"name": "Protection", "description": "Models in this unit get +1 Save.", "points": 20}
    },
    "strategies": {
        "covering_fire": {
            "name": "Covering Fire",
            "phase": "activation",
            "flavor": "Members of the Federation are tirelessly drilled on covering one another in the event that one unit is overrun.",
            "description": "Use when a friendly _Infantry_ or _Heavy Infantry_ unit is Charged. One friendly _Infantry_ or _Heavy Infantry_ unit within 6\" may take an Initiative test and attempt to react to the charging unit.",
            "cost": 1
        },
        "preservation_protocols": {
            "name": "Preservation Protocols",
            "phase": "activation",
            "flavor": "Federation drones are programmed to protect their creators at all costs.",
            "description": "Use after an _Infantry_ or _Heavy Infantry_ unit has been hit by a ranged weapon. For this attack, you can roll a D10 each time an attack made with a ranged weapon hits that unit. If there is a friendly _Drone_ unit within 6\" of it, and the _Drone_ unit is closer to the attacking model than the target unit. On an 8 or less a hit is transferred to the _Drone_ unit instead of the original target.",
            "cost": 2
        },
        "spotting_laser_uplink": {
            "name": "Spotting Laser Uplink",
            "phase": "activation",
            "flavor": "Some units equipped with spotting lasers are connected to a laser-uplink which boosts the signal of nearby spotting lasers.",
            "description": "Use after a unit successfully marks a unit with a spotting laser. You may place one additional spotting marker on the target.",
            "cost": 1
        }
    },
