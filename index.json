                {"type": "grantRule", "rule": "ambush"},
                {"type": "changeStat", "statName": "defense", "value": 8},
                {"type": "alterStat", "statName": "wounds", "value": 2},
                {"type": "alterStat", "statName": "movement", "value": -1}
            ]
        },
