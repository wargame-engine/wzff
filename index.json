        "banner_of_focus": {"name": "Banner of Focus", "description": "Once per round, a friendly _Infantry_ unit within 6\" gains +1 AP on all Shooting weapons.", "points": 10},
        "poison": {"name": "Poison", "description": "Each successful hit roll of 4 or less with this weapon gets +1 AP.", "points": ["weapon.attacks", {"multiply": 0.5}]},
        "lance": {"name": "Lance", "description": "This weapon gets +1 AP when used during a Charge.", "points": ["weapon.attacks", {"multiply": 1}]},
        "dozer": {"name": "Dozer", "description": "This model may cross low obstacles without penalty.", "points": 5},
        "shielding": {"name": "Shielding", "description": "Whenever this model loses a wound, roll a D10. On a 3 or less that wound is not lost.", "points": 5},
        "shield": {"name": "Shield", "description": "Whenever this model loses a wound, roll a D10. On a 3 or less that wound is not lost.", "points": 5},
        "psych_out": {"name": "Dangerous(Psychic)", "description": "This weapon has +1 AP against units with the Psychic special rule."},
        "dark_prism": {
            "name": "Dark Prism",
            "description": "Once per round, when a model with Psychic(x) casts a power within 24\", roll a D10. If the die roll is lower than or equal to the casting roll, the power is prevented.",
            "points": 5
        },
        "medical_training": {
            "name": "Medical Training",
            "description": "Whenever this model, or a friendly _Infantry_ model within 6\" is killed or loses a Wound(x), roll a D10. On a 3 or less, that loss is prevented.",
            "points": 15
        },
        "ward": {
            "name": "Ward",
            "description": "Once per round, when a model with Psychic(x) casts a power within 24\", roll a D10. If the die roll is lower than or equal to the casting roll, the power is prevented.",
            "points": 5
        },
        "legend_wounds": {"name": "Resilient", "description": "This model gains Wounds(+1).", "points": 10},
        "legend_courage": {"name": "Inspired", "description": "This model gains +1 Courage.", "points": 10},
        "gravity": {"name": "Gravity", "description": "This weapon gets +2 AP against targets with Save 10 or higher.", "points": ["weapon.attacks", {"multiply": 1}]},
        "homing": {"name": "Homing", "description": "This weapon hits at Accuracy +1.", "points": ["weapon.attacks"]},
        "titanic": {
            "name": "Titanic",
            "description": "When this unit is activated it may only perform one action and benefit from abilties which trigger when the unit is activated once. It may then be activated a second time to perform its second action."
        },
        "spiked_ram": {"name": "Spiked Ram", "hidden": true, "description": "This model gains Impact(+3).", "description_short": "Impact +3", "points": 6},
        "heavy_impact": {
            "name": "Heavy Impact",
            "inputs": ["x"],
            "description": "When this model successfully charges an enemy unit, make X Strength tests. For each success, the enemy takes 1 hit with AP 1.",
            "points": ["model.movement", {"divide": 12}, {"add": ["model.fight", {"subtract": "constants.baseStats.fight"}]}, {"add": 1}, {"multiply": "rule.x"}]
        },
        "emp": {"name": "EMP", "description": "Successful hit rolls of 5 or less gain +2 AP.", "points": ["weapon.attacks", {"multiply": 4}, {"multiply": 0.5}]},
        "jetpack_character": {
            "name": "Jetpack",
            "hidden": true,
            "description_short": "Fly, Ambush, Movement +2\"",
            "description": "Increases movement by 2\". Gives the model fly and ambush.",
            "points": 15
        },
        "jetpack": {
            "name": "Jetpacks",
            "description": "This model's movement is increased by 2\". This model may cross all terrain as if it was not there. This unit may be set aside in reserves before the match begins if all models have ambush. Any round after the first, when this unit is activated while in reserves, you may choose a point on the battlefield 12\" away from any enemy units and take an Initiative check. If passed, place the unit at that location on the location. If failed, use D10 scattering to determine the landing location of the unit relative to the chosen point.",
            "points": 4
        },
        "overheat": {"name": "Overheat", "description": "The firer must take 1 save for each hit roll of 9+.", "points": ["weapon.attacks", {"multiply": -2}]},
        "destroyer_armor": {
            "name": "Destroyer Armor",
            "hidden": true,
            "description_short": "Ambush, Save +1, Movement -1\"",
            "description": "Gives the model the ambush keyword. Gives the model Save 8.",
            "points": 15
        },
        "field_radio": {
            "name": "Field Radio",
            "description": "This unit may be targeted by the _Field Orders_ special rule if it is within 24\" of another unit with a _Field Radio_ that is in range of the _Field Orders_ rule.",
            "points": 5
        },
        "field_radio_stormtrooper": {
